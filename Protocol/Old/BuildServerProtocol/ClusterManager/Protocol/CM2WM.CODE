package Protocol.ClusterManager.CM2WM;

//===================================================================
//===============ClusterManager=>WorldManager======0x1300000=========
//===================================================================

message MSG_CM2WM_HEARTBEAT 0x1300001
{
	required int32 GroupId = 1;
	required int32 SubId = 2;
}

message MSG_CM2WM_RETRUN_REGISTER 0x1300002
{
	required int32 GroupId = 1;
	required int32 SubId = 2;
}