message MSG_BM2B_HEARTBEAT 0x5600001
{
	required int32 GroupId = 1;
	required int32 SubId = 2;
}

message MSG_BM2B_RETRUN_REGISTER 0x5600002
{
	required int32 GroupId = 1;
	required int32 SubId = 2;
}