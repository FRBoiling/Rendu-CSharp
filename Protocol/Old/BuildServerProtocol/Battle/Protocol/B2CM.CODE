message MSG_B2CM_Heartbeat 0x6100001
{
	required int32 GroupId = 1;
	required int32 SubId = 2;
}

message MSG_B2CM_Register 0x6100002
{
	required int32 GroupId = 1;
	required int32 SubId = 2;
}