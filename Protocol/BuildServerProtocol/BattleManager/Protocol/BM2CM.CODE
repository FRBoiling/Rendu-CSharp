package Protocol.BattleManager.BM2CM;

//===================================================================
//===============battle manager -> cluster manager ==0x5100000======
//===================================================================

message MSG_BM2CM_HEARTBEAT 0x5100001
{
	required int32 GroupId = 1;
	required int32 SubId = 2;
}

message MSG_BM2CM_REGISTER 0x5100002
{
	required int32 GroupId = 1;
	required int32 SubId = 2;
}