package Protocol.ClusterManager.CM2B;

//===================================================================
//=============== cluster manager -> battle ======0x1600000=========
//===================================================================

message MSG_CM2B_HEARTBEAT 0x1600001
{
	required int32 GroupId = 1;
	required int32 SubId =2;
}

message MSG_CM2B_RETRUN_REGISTER 0x1600002
{
	required int32 GroupId = 1;
	required int32 SubId =2;
}