package Protocol.Battle.B2BM;

//===================================================================
//======================== battle -> battle manager =====0x6500000===
//===================================================================

message MSG_B2BM_HEARTBEAT 0x6500001
{
	required int32 GroupId = 1;
	required int32 SubId = 2;
}

message MSG_B2BM_REGISTER 0x6500002
{
	required int32 GroupId = 1;
	required int32 SubId = 2;
}
